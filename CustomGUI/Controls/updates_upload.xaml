<Window x:Class="CustomGUI.Controls.updates_upload"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CustomGUI.Controls"
        mc:Ignorable="d"
        Title="ProgressBar Demo" SizeToContent="Height" Width="525">
    <StackPanel>
        <StackPanel>
            <Grid>
                <Image Source="/img/upload.png" Height="20" Width="20"  HorizontalAlignment="Left"/>
                <Label Name="ProgressLabel" HorizontalAlignment="Left" Margin="20,0,0,0">Exporting</Label>
            </Grid>
        </StackPanel>
        <ProgressBar Name="pgb" Height="20" Minimum="0" Maximum="100" Margin="5"/>
    </StackPanel>
</Window>
